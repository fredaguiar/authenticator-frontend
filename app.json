{
  "expo": {
    "name": "authenticator-frontend",
    "slug": "authenticator-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "'com.fredaguiar.authenticatorfrontend'",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fredaguiar.authenticatorfrontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-secure-store", "@react-native-google-signin/google-signin"],
    "extra": {
      "eas": {
        "projectId": "8ffb2a19-611b-4fa3-879c-4dbbcc1dd60a"
      }
    }
  }
}
